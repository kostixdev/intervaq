{
  "name": "intervaq",
  "version": "1.0.1",
  "description": "Just another one solution for intervals and timeouts via requestAnimationFrame",
  "main": "src/index.js",
  "scripts": {
    "prepare": "husky install",
    "test": "jest && npm run test:badges",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "jest-coverage-badges --input ./coverage/coverage-summary.json --output ./badges/jest",
    "lint:check": "eslint ./src/*",
    "lint:fix": "eslint --fix ./src/*",
    "pretty:fix": "npx prettier ./test/index.test.js ./src/index.js --write",
    "build:docs": "node node_modules/.bin/jsdoc2md --heading-depth 1 ./src/index.js > ./docs/README.md",
    "license:check": "jsgl --local ."
  },
  "keywords": [
    "setInterval",
    "setTimeout",
    "requestAnimationFrame"
  ],
  "author": "kostix.dev",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.9",
    "babel-jest": "^29.6.1",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "git-conventional-commits": "^2.6.5",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.6.2",
    "js-green-licenses": "^4.0.0",
    "jsdoc-to-markdown": "^8.0.0",
    "prettier": "3.0.0"
  }
}
